    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "madge": "^5.0.1",
    "prettier": "^2.4.1",
