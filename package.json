{
  "name": "@stellarbeat/js-stellarbeat-backend",
  "private": true,
  "version": "0.5.0",
  "description": "Backend & api",
  "repository": {
    "type": "git",
    "url": "https://github.com/stellarbeat/js-stellarbeat-backend.git"
  },
  "scripts": {
    "build": "tsc --build && cp -R src/notifications/infrastructure/templates lib/notifications/infrastructure/templates",
    "test:unit": "jest",
    "test:integration": "jest -c jest.integration.config.js --runInBand",
    "test": "yarn test:unit; yarn test:integration",
    "heroku-postbuild": "yarn run build",
    "build-and-update-network": "yarn run build; node lib/network/infrastructure/cli/update-network.js",
    "update-network": "node lib/network/infrastructure/cli/update-network.js",
    "build-and-start-api": "yarn run build; node lib/core/infrastructure/http/api.js",
    "build-and-seed-db": "yarn run build; node lib/network/infrastructure/cli/seed-db",
    "build-and-start-history-verification": "yarn run build; node lib/history-scan/infrastructure/cli/verify-archives.js",
    "start-history-verification": "node lib/history-scan/infrastructure/cli/verify-archives.js",
    "seed-db": "node lib/network/infrastructure/cli/seed-db",
    "init-rollup": "node lib/network/infrastructure/cli/init-rollup-table",
    "start-api": "node lib/core/infrastructure/http/api.js",
    "retrieve-home-domain": "node lib/network/infrastructure/cli/retrieve-home-domain.js"
  },
  "author": "pieterjan84@github",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@sentry/node": "^6.15.0",
    "@stellarbeat/js-stellar-domain": "^4.1.2",
    "@stellarbeat/js-stellar-node-crawler": "^4.0.0-0",
    "@stellarbeat/stellar-history-archive-hasher": "^0.2.0",
    "@stellarbeat/stellar_analysis_nodejs": "^0.6.2",
    "@types/workerpool": "^6.1.0",
    "ajv": "^8.11.0",
    "async": "^3.2.4",
    "await-fs": "^1.0.0",
    "aws-sdk": "^2.993.0",
    "axios": "1.1.0",
    "blocked-at": "^1.2.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.5.1",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "helmet": "^4.6.0",
    "inversify": "^5.1.1",
    "lru-cache": "^6.0.0",
    "mathjs": "^10.5.0",
    "moment": "^2.29.4",
    "neverthrow": "^6.0.0",
    "pg": "^8.7.1",
    "pg-query-stream": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "shallow-equal-object": "^1.1.1",
    "swagger-ui-express": "^4.4.0",
    "toml": "^3.0.0",
    "typeorm": "0.2.38",
    "uuid": "^8.3.2",
    "validator": "^13.7.0",
    "winston": "^3.1.0",
    "workerpool": "^6.3.0",
    "yn": "^4.0.0"
  },
